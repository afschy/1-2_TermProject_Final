<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.PlayerInfoTable">
   <children>
      <Label layoutX="360.0" layoutY="26.0" text="Results:">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <TableView fx:id="tableView" layoutX="26.0" layoutY="120.0" prefHeight="461.0" prefWidth="751.0">
         <columns>
            <TableColumn fx:id="nameCol" prefWidth="171.20001220703125" text="Name" />
            <TableColumn fx:id="countryCol" prefWidth="67.199951171875" text="Country" />
            <TableColumn fx:id="ageCol" prefWidth="41.5999755859375" text="Age" />
            <TableColumn fx:id="heightCol" prefWidth="76.0" text="Height" />
            <TableColumn fx:id="clubNameCol" prefWidth="145.60003662109375" text="Club Name" />
            <TableColumn fx:id="positionCol" prefWidth="86.39996337890625" text="Position" />
            <TableColumn fx:id="jerseyNumberCol" prefWidth="70.4000244140625" text="Number" />
            <TableColumn fx:id="salaryCol" prefWidth="86.39990234375" text="Salary" />
         </columns></TableView>
      <Label fx:id="counter" layoutX="26.0" layoutY="69.0" prefHeight="30.0" prefWidth="394.0" text="Label" textAlignment="JUSTIFY">
