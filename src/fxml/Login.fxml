<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.Login">
   <children>
      <Label layoutX="55.0" layoutY="62.0" prefHeight="34.0" prefWidth="114.0" text="Club Name:">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <TextField fx:id="clubNameField" layoutX="226.0" layoutY="57.0" prefHeight="26.0" prefWidth="322.0">
         <font>
            <Font size="20.0" />
         </font>
      </TextField>
      <Button fx:id="logInButton" layoutX="254.0" layoutY="236.0" mnemonicParsing="false" onAction="#logInButtonPressed" prefHeight="43.0" prefWidth="92.0" text="Log In">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
      <Button fx:id="exitButton" layoutX="254.0" layoutY="302.0" mnemonicParsing="false" onAction="#exitButtonPressed" prefHeight="43.0" prefWidth="92.0" text="Exit">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
